{
    "name": "teknoo/paypal-exprss",
    "type": "library",
    "description": "Library to perform quickly sales with the service Paypal Express Checkout",
    "keywords": ["paypal","express"],
    "homepage": "http://teknoo.software/paypal-express",
    "license": ["BSD-3-Clause"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richard@teknoo.software",
            "role": "Software architect, Lead developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TeknooSoftware/paypal-express/issues",
        "source": "https://github.com/TeknooSoftware/paypal-express"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/teknoo_software"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/TeknooSoftware"
        }
    ],
    "require":{
        "php": "^8.4",
        "psr/http-message":"^2",
        "psr/http-factory":"^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^13.0.1",
        "squizlabs/php_codesniffer": "^4.0.1",
        "phpstan/phpstan": "^2.1.38",
        "symfony/property-access": "^6.4||^7.4||^8",
        "php-http/client-common": "^2.7.3",
        "php-http/curl-client": "^2.4",
        "php-http/message": "^1.16.2",
        "php-http/discovery": "^1.20",
        "nyholm/psr7": "^1.8.2"
    },
    "autoload": {
        "psr-4": {
            "Teknoo\\Paypal\\Express\\": ["src/Paypal/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teknoo\\Tests\\Paypal\\": ["tests/Paypal"]
        }
    },
    "archive": {
        "exclude": ["demo", "vendor", ".idea"]
    },
    "config": {
        "audit": {
            "abandoned": "report"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
